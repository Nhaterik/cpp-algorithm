#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        string s1,s2,res="";
        cin >> s1>>s2;
       int l1=s1.size(),l2=s2.size();
       if(l1<l2 || (s1<s2 && l1==l2)) swap(s1,s2);
       l1=s1.size(),l2=s2.size();
       int i=0,j=0,s=0,remainder=0;
       s2=string(l1-l2,'0')+s2;
       l2=s2.size();
       reverse(s1.begin(),s1.end());
       reverse(s2.begin(),s2.end());
        while(i<l1)
        {
            s=s1[i]-(s2[j]+remainder);
            if(s<0) 
            {
                remainder=1;
                s+=10;
                
            }
            else 
            {
                remainder=0;
                
            }
            res+=s+'0';
            i++,j++;
        }
        reverse(res.begin(),res.end());
        cout<<res<<endl;
    }
    return 0;
}
...........................
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        string s1,s2,res="";
        cin >> s1>>s2;
       int l1=s1.size(),l2=s2.size();
       if(l1<l2 || (l1==l2 && s1<s2)) swap(s1,s2);
       int i=0,j=0,s=0,remainder=0;
       l1=s1.size(),l2=s2.size();
       s2=string(l1-l2,'0')+s2;
       l2=s2.size();
       reverse(s1.begin(),s1.end());
       reverse(s2.begin(),s2.end());
        while(i<l1 )
        {
            s=s1[i]-'0'+s2[j]-'0'+remainder;
            if(s<10) 
            {
                remainder=0;
                
            }
            else 
            {
                s=s%10;
                remainder=1;
                
            }
            res+=s+'0';
            i++,j++;
        }
        // case 1+9=10
        if(remainder) res+='1';
        reverse(res.begin(),res.end());
        cout<<res<<endl;
    }
    return 0;
}
